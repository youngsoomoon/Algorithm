    "n = int(input())\n",
    "data = list(map(int,input().split()))\n",
    "\n",
    "data.sort()\n",
    "target = 1\n",
    "for x in data:\n",
    "    if target < x:\n",
    "        print('break - data:',x,'target:',target)\n",
    "        break\n",
    "    target += x\n",
    "    print('data:',x,'target:',target)\n",
    "    \n",
    "print(target)"
